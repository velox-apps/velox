{
  "$schema": "https://velox.dev/schema/velox.schema.json",
  "productName": "HelloWorld2",
  "version": "1.0.0",
  "identifier": "com.velox.helloworld2",
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Welcome to Velox! (Bundled Assets)",
        "width": 800,
        "height": 600,
        "url": "app://localhost/",
        "create": true,
        "visible": true,
        "focus": true,
        "resizable": true,
        "decorations": true,
        "customProtocols": ["app", "ipc"]
      }
    ],
    "macOS": {
      "activationPolicy": "regular"
    }
  },
  "build": {
    "frontendDist": "assets"
  }
}
