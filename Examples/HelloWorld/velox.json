{
  "$schema": "https://velox.dev/schema/velox.schema.json",
  "productName": "HelloWorld",
  "version": "1.0.0",
  "identifier": "com.velox.helloworld",
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Welcome to Velox!",
        "width": 800,
        "height": 600,
        "url": "app://localhost/",
        "create": true,
        "visible": true,
        "focus": true,
        "resizable": true,
        "decorations": true,
        "customProtocols": ["app", "ipc"]
      }
    ],
    "macOS": {
      "activationPolicy": "regular"
    }
  },
  "bundle": {
    "active": true,
    "targets": ["app"],
    "macos": {
      "minimumSystemVersion": "13.0"
    }
  }
}
